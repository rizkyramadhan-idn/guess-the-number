{
  "name": "guess-the-number",
  "version": "0.0.1",
  "description": "Guess the number game",
  "scripts": {
    "start": "npm run clean && npm run webpack-dev",
    "build": "npm run clean && npm run webpack-build",
    "clean": "node task/clean",
    "webpack-build": "NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --https --inline --hot --colors --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/widatama/nub.git"
  },
  "keywords": [
    "web",
    "application",
    "number",
    "guesssing"
  ],
  "author": "Danu Widatama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/widatama/guess-the-number/issues"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-exclude-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-detect-browsers": "^2.2.5",
    "karma-tap": "^3.1.1",
    "karma-tap-pretty-reporter": "^3.0.5",
    "karma-webpack": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "pug": "^2.0.0-beta.12",
    "pug-loader": "^2.3.0",
    "tap-summary": "^3.0.2",
    "tape": "^4.6.3",
    "vue-hot-reload-api": "^2.1.0",
    "vue-loader": "^12.0.4",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "localforage": "^1.5.3",
    "normalize.css": "^7.0.0",
    "vue": "^2.4.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  }
}
