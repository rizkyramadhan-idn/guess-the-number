{
  "name": "nub",
  "version": "0.0.1",
  "description": "Web application development starter pack",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack-dev",
    "build": "npm run clean && npm run webpack-build",
    "clean": "node task/clean",
    "webpack-build": "NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --https --inline --hot --colors --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/widatama/vue-starter.git"
  },
  "keywords": [
    "vue",
    "starter"
  ],
  "author": "Danu Widatama",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/widatama/vue-starter/issues"
  },
  "homepage": "https://gitlab.com/widatama/vue-starter#README",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "pug": "^2.0.0-beta.12",
    "pug-loader": "^2.3.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "normalize.css": "^7.0.0"
  }
}
